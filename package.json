{
    "name": "elo-league",
    "type": "module",
    "version": "1.0.0",
    "scripts": {
        "build": "bun --bun vite build",
        "check": "bun run typecheck && bun run prettier:check && bun run lint:check",
        "db": "docker compose up",
        "dev": "bun --bun vite dev",
        "lint": "bun --bun eslint . --fix",
        "lint:check": "bun --bun eslint .",
        "migrate": "bun run --bun database/migrate.ts",
        "prettier": "prettier --write .",
        "prettier:check": "prettier --check .",
        "seed": "bun run --bun database/seed.ts",
        "serve": "bun run --bun server.ts",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@eslint/js": "^9.37.0",
        "@types/bun": "^1.2.23",
        "@types/react": "^19.2.0",
        "@types/react-dom": "^19.2.0",
        "@vitejs/plugin-react": "^5.0.4",
        "eslint": "^9.37.0",
        "eslint-plugin-react": "^7.37.5",
        "globals": "^16.4.0",
        "prettier": "3.6.2",
        "typescript-eslint": "^8.45.0",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.14",
        "@tanstack/react-router": "^1.132.33",
        "@tanstack/react-router-devtools": "^1.132.33",
        "@tanstack/react-start": "^1.132.35",
        "@tanstack/zod-adapter": "^1.132.33",
        "@types/pg": "^8.15.5",
        "kysely": "^0.28.7",
        "pg": "^8.16.3",
        "prettier-plugin-tailwindcss": "^0.6.14",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "tailwindcss": "^4.1.14",
        "vite": "^7.1.9",
        "zod": "^4.1.11"
    }
}
