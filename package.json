{
    "name": "elo-league",
    "type": "module",
    "version": "1.0.3",
    "scripts": {
        "build": "bun --bun vite build",
        "check": "bun run typecheck && bun run prettier:check && bun run lint:check",
        "db": "docker compose up",
        "dev": "bun --bun vite dev",
        "lint": "bun --bun eslint . --fix",
        "lint:check": "bun --bun eslint .",
        "migrate": "bun run --bun database/migrate.ts",
        "prettier": "prettier --write .",
        "prettier:check": "prettier --check .",
        "seed": "bun run --bun database/seed.ts",
        "serve": "bun run --bun server.ts",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.0",
        "@types/bun": "^1.3.1",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "^5.1.0",
        "eslint": "^9.39.0",
        "eslint-plugin-react": "^7.37.5",
        "globals": "^16.4.0",
        "prettier": "3.6.2",
        "typescript-eslint": "^8.46.2",
        "vite-tsconfig-paths": "^5.1.4"
    },
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.16",
        "@tanstack/react-router": "^1.134.4",
        "@tanstack/react-router-devtools": "^1.134.4",
        "@tanstack/react-start": "^1.134.4",
        "@tanstack/zod-adapter": "^1.134.4",
        "@types/pg": "^8.15.6",
        "kysely": "^0.28.8",
        "pg": "^8.16.3",
        "prettier-plugin-tailwindcss": "^0.7.1",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "tailwindcss": "^4.1.16",
        "vite": "^7.1.12",
        "zod": "^4.1.12"
    }
}
